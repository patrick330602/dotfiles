                    function checkUTFEncoding() {
                        var decodedString = "你好A";
                        if (htmlDec("&#20320;&#22909;&#65;") != decodedString) {
                            alert("Outlook Web App 要求您的瀏覽器必須使用 UTF-8 編碼。為了保護您，現在會將您登出。請將瀏覽器設定為 UTF-8 編碼，然後重新登入。");
                        window.location.href = "/owa/logoff.owa";
                    }
                }

                checkUTFEncoding();

                userScriptResources[userScriptResources.length] = { "fileName": "prem/15.0.1076.11/scripts/##LANG##/boot.mouse.strings.localized.min.js", "layout": LT_ANY };
                

                    // initialize boot scripts load states
                    window.scriptsLoaded = window.scriptsLoaded || {};
                    window.scriptsLoaded['preboot.js'] = 0;

                
                    window.scriptsLoaded['boot.0.mouse.js'] = 0;
                
                    window.scriptsLoaded['boot.1.mouse.js'] = 0;
                
            }
            catch (ex) {
                window.owaLastErrorReported = ex;
                throw ex;
            }

            function setupUserSpecificResources(serverVersion, curTheme, curCulture, curLang, isRtl, minOwsVersion, serverOwsVersion, curStyleLocale, userSpecificResourcesHash, curFeatures, curDefaultAspxVer) {
                try{
                    var defaultPageVer = 3;
                    var forcePLTOnVersionChange = true;
                    var versionCheckPassed = defaultPageVer == curDefaultAspxVer;

                    if (forcePLTOnVersionChange) {
                        versionCheckPassed = versionCheckPassed && serverVersion == sver;
                    }
                    else if (minOwsVersion == null) {
                        // this will be the case for users not on the OwaVersioning flight
                        versionCheckPassed = versionCheckPassed && !isMajorVersionChanged(sver, serverVersion);
                    }
                    else {
                        var clientVer = _g.OWSVersion.create(_g.ClientVersionProvider.currentOWSVersion);
                        var serverVer = _g.OWSVersion.create(serverOwsVersion);
                        var minVer = _g.OWSVersion.create(minOwsVersion);
                        versionCheckPassed = versionCheckPassed && clientVer.supports(minVer) && serverVer.supports(clientVer);
                    }

                    var compareFeatures = function(cf, f) {
                        if ((cf == null && f == null) || cf == f)
                            return true;
                        if (cf == null || f == null || cf.length != f.length)
                            return false;
                        for (var i = 0; i < cf.length; i++) {
                            if (cf[i] != f[i]) return false;
                        }
                        return true;
                    }

                    var featureCheckPassed = compareFeatures(curFeatures, featuresVar);
                    var redirectParam = 'ver' + '=' + serverVersion;
                    if (versionCheckPassed && featureCheckPassed &&
                            (serverVersion == sver || 
                            (curTheme == clientTheme && curCulture == userCultureVar && curLang == userLanguageVar && userSpecificResourcesHash == userSpecificResourcesHashVar))|| 
                            window.location.href.indexOf(redirectParam) >=0) {
                        clientTheme = curTheme;
                        userCultureVar = curCulture;
                        userLanguageVar = curLang;
                        userCultureRtl = isRtl;
                        stylesLocale = curStyleLocale;

                        document.getElementsByTagName("html")[0].setAttribute('dir', isRtl ? "rtl" : "ltr");


                        
                            loadScripts(userScriptResources, cdnEndPointNameForBootResources != "", false);
                        
                    }
                    else {
                        redirect('ver', serverVersion,'cver', sver, 'cf', (featureCheckPassed ? 1 : 0), 'vC', (versionCheckPassed ? 1 : 0));
                    }
                }
                catch (ex) {
                    window.owaLastErrorReported = ex;
                    throw ex;
                }
            }
        </script>
        
