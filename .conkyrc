background no
use_xft yes
xftfont Ubuntu:size=12
update_interval 3.0
total_run_times 0
own_window yes
own_window_type panel
own_window_transparent no
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
own_window_argb_visual yes
own_window_argb_value 77
double_buffer yes
minimum_size 3000 0
maximum_width 3000
draw_shades no
draw_outline no
draw_borders no
draw_graph_borders yes
default_color white
default_shade_color black
alignment top_middle
gap_x 0
gap_y 69
no_buffers yes$
cpu_avg_samples 2
override_utf8_locale no
uppercase false

TEXT
Uptime: $uptime || CPU | 1:${cpu cpu1}% 2:${cpu cpu2}% 3:${cpu cpu3}% 4:${cpu cpu4}% ${cpugraph 20,250} || Load: $loadavg | Processes: $processes | Running: $running_processes | RAM: $mem/$memmax ${membar 20,120} | Swap: $swap/$swapmax ${swapbar 20,120}
Write: $diskio_write ${diskiograph_write 20,100} | Read: $diskio_read ${diskiograph_read 20,100} || ROOT ${fs_bar 20,100 /} | HOME ${fs_bar 20,100 /home} || Download:${color #ddaa00} Speed: ${downspeed wlp3s0}/s$color Tot: ${totaldown wlp3s0} ${downspeedgraph wlp3s0 20,100} | Upload:${color #ddaa00} Speed: ${upspeed wlp3s0}/s$color Tot: ${totalup wlp3s0} ${upspeedgraph wlp3s0 20,100}
